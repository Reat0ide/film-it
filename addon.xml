<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.pelisalacarta"
    name="pelisalacarta"
    version="4.0.1"
    provider-name="Reat0ide">
  <requires>
    <import addon="xbmc.python" version="2.19.0"/>
    <import addon="script.module.urlresolver"  version="0.0.1"/>
    <import addon="script.module.selenium" version="1.0.0"/>
    <import addon="script.module.elementtree" version="1.2.8"/>
      <import addon="script.module.beautifulsoup4" version="4.3.2"/>
      <import addon="script.module.html5lib" version="0.999.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Movies and TV Shows on demand</summary>
    <description lang="en">Movies, Series, Documentaries, Anime, ... everything you can find on Internet now on your media center.</description>
    <platform>all</platform>
  </extension>
  <extension point="xbmc.service" library="library_service.py" start="login|startup">
  </extension>
</addon>
